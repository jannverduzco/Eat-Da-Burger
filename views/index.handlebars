<h1>Eat-Da-Burger!</h1>




<ul>
  {{#each burgers}}
    {{#unless devoured}}
   <li>
	{{burger_name}}

	<button class="change-devoured" data-id="{{id}}" data-newdevoured="{{devoured}}">
		{{#if devoured}}SLEEP TIME!{{else}}WAKE UP!{{/if}}
	</button>

	<button class="delete-cat" data-id="{{this.id}}">DELETE!</button>
</li>
    {{/unless}}
  {{/each}}
</ul>